{
	"name": "New Tab Countdown v2 - With dark mode",
	"description": "Countdown time remaining in the day, week, month, year, or a custom date. Now with dark mode.",
	"version": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["storage"],
	"content_security_policy": {
		"newtab": "script-src 'self'; object-src 'self'",
		"extension_pages": "script-src 'self'; object-src 'self'"
	}
}
